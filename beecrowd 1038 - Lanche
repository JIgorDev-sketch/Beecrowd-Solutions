#include <stdio.h>
int main(){
	int code;
	double quantity;
	
	scanf("%d %lf", &code, &quantity);
	
	switch(code){
		case 1: printf("Total: R$ %.2lf\n", quantity * 4); break;
		case 2: printf("Total: R$ %.2lf\n", quantity * 4.5); break;
		case 3: printf("Total: R$ %.2lf\n", quantity * 5); break;
		case 4: printf("Total: R$ %.2lf\n", quantity * 2); break;
		case 5: printf("Total: R$ %.2lf\n", quantity * 1.5); break;
	}
	
return 0;
}
