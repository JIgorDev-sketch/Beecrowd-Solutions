#include <stdio.h>
int main(){
	int code1, number1, code2, number2;
    double unit_value1, unit_value2;
    
    scanf("%d %d %lf", &code1, &number1, &unit_value1);
    scanf("%d %d %lf", &code2, &number2, &unit_value2);
     
	printf("VALOR A PAGAR: R$ %.2lf\n", number1 * unit_value1 + number2 * unit_value2);
	
return 0;	
}
